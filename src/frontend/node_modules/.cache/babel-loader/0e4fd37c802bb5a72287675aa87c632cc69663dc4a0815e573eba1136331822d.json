lect-size label{\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n.main-wrap .book .book-details .sizes .form .select-size label input{\r\n    display: none;\r\n}\r\n.main-wrap .book .book-details .sizes .form .select-size label span{\r\n    font-size: 18px;\r\n    font-weight: 400;\r\n    line-height: 25px;\r\n    text-transform: uppercase;\r\n    color: #011627;\r\n    display: inline-block;\r\n    margin-right: 10px;\r\n    cursor: pointer;\r\n}\r\n.main-wrap .book .book-details .sizes .form .select-size label input:checked + span{\r\n    font-weight: 600;\r\n    color: #00b4d8;\r\n}\r\n.genres{\r\n    margin-bottom: 20px;\r\n}\r\n.genres .genre{\r\n  border-radius: 10%;\r\n  border: solid, 1px, black;\r\n  font-size: small;\r\n}\r\n.main-wrap .book .book-details .quantity{\r\n    margin-bottom: 50px;\r\n}\r\n.main-wrap .book .book-details .quantity .select-quantity{\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n.main-wrap .book .book-details .quantity .select-quantity h3{\r\n    width: 70px;\r\n    margin-right: 30px;\r\n    font-size: 20px;\r\n    font-weight: 500;\r\n    color: #011627;\r\n}\r\n.main-wrap .book .book-details .quantity .select-quantity input{\r\n    background: #e8e8e8;\r\n    border: 0;\r\n    outline: 0;\r\n    padding: 2px 2px 2px 15px;\r\n    border-radius: 12px;\r\n    width: 50px;\r\n}\r\n.main-wrap .book .book-details .sub-btn{\r\n    padding: 0 30px;\r\n}\r\n.main-wrap .book .book-details .sub-btn .submit{\r\n    width: 100%;\r\n    padding: 10px;\r\n    border: 0;\r\n    outline: 0;\r\n    background: #0077b6;\r\n    color: #fff;\r\n    font-size: 15px;\r\n    font-weight: 500;\r\n    border-radius: 30px;\r\n    box-shadow: 0 10px 10px rgba( 85, 63, 240, 0.25 );\r\n    cursor: pointer;\r\n    transition: .4s linear;\r\n    position: relative;\r\n    overflow: hidden;\r\n}\r\n.main-wrap .book .book-details .sub-btn .submit:hover{\r\n    background: #00b4d8;\r\n}\r\n.main-wrap .book .book-details .sub-btn .submit::be